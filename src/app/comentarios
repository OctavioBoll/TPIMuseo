  //conocerEmpleado()
  buscarEmpleado(){
    for (let index = 0; index < this.empleado.length; index++) {
      //buscar en todos los empleados el empleado que tiene el idEmpleado de usuario seleccionado
      if(this.auxUsuarioSelect[0].idEmpleado == this.empleado[index].id){
